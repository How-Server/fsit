{
	"schemaVersion": 1,
	"id": "fsit",
	"version": "${version}",
	"name": "FSit",
	"description": "Sit anywhere!",
	"authors": [
		"rvbsm"
	],
	"contact": {
		"sources": "https://github.com/rvbsm/fsit",
		"issues": "https://github.com/rvbsm/fsit/issues"
	},
	"license": "MIT",
	"icon": "assets/fsit/icon.png",
