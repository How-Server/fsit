		"main": [
			"dev.rvbsm.fsit.FSitMod"
		],
		"client": [
			"dev.rvbsm.fsit.FSitClientMod"
		],
		"server": [
			"dev.rvbsm.fsit.FSitServerMod"
		],
		"modmenu": [
			"dev.rvbsm.fsit.FSitModMenu"
		]
	},
	"mixins": [
		"fsit.mixins.json",
		{
			"config": "fsit.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.0"
	}
